    {% block style %}{% endblock %}

    <title>{% block title %}jan peterka{% endblock %}</title>
{% endblock head %}
</head>
<body>
    {% block navbar %}
        {% include('navbar.html.j2') %}
    {% endblock %}
    
    <div class="container mt-5">
